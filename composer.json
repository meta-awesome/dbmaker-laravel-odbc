{
	"name": "metawesome/laravel-odbc",
	"description": "DBMaker integration for Laravel framework ",
	"keywords": [
		"laravel",
		"DBMaker",
		"Database",
		"PDO",
		"ODBC"
	    ],
	"type": "library",
    	"license": "MIT",
	"homepage": "http://www.dbmaker.com.tw",
	"authors": [
		{
			"name": "syscom",
			"email": "info@syscom.com.tw",
			"homepage": "http://www.dbmaker.com.tw",
			"role": "Developer"
		}
